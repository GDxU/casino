var keepAliveURL = "https://www.mrgreen.com/"; 

// Function:	getKeepAliveURL
// Parameters:	void
// Return:		String
function getKeepAliveURL()
{
	if(keepAliveURL.length > 0 && keepAliveURL != "null"){
		return keepAliveURL + "?" + new Date();
	}			
}

// Function:	keepAlive
// Parameters:	void
// Return:		void
function keepAlive()
{
	if(document.getElementById('keepAliveImage') != null){				
		document.getElementById('keepAliveImage').src = getKeepAliveURL();
	}
}

var baseCloseGame = closeGame;

// Function:	closeGame
// Parameters:	void
// Return:		void
function closeGame()
{
	if (window.top.frames[0].name == 'content'){
		top.close();
 	}
}

