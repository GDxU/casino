<?xml version="1.0" encoding="utf-8"?>
<SlotMachine version="5.1.0.8">
	<init>
		<console id="2000-0" />
		<asset id="lokiFrameBG" frame="" />
		<asset id="lokiBG" frame="" />
	</init>
	
	<Delays>
		<Delay id="minWildStormDelay" value="3000" />
		<Delay id="minSpinTime" value="950" />
		<Delay id="minQuickSpinTime" value="550" />
		<Delay id="timeBetweenReelStops_1" value="150" />
		<Delay id="timeBetweenReelStops_2" value="800" />
		<Delay id="timeBetweenReelStops_3" value="1600" />
		<Delay id="timeQuickSpinInterval" value="0" />
		<Delay id="timeDelayForWinDisplay" value="250" />
		<Delay id="timeDelayForMinWait" value="250" />
	</Delays>
	
	<ModalHeaders>
		<Automation enabled="0" />
		<DefaultHeader id="lokiLogo" />
		
		<ConditionalHeaders>
			
			<ConditionalBag id="5ofKind" csvWinIds="0,3,6,9,12,15,18,21,24,27,30,33,36,39,42,45,48,51,54,57,60,63,66,69,72" duration="1">
				<!-- id is the ID of the MovieClips Export linkage and the ID as used in the Art Asset Controller -->
				<!-- It is possible to have multiple Headers in a conditionla bag as per the Random Bag -->
				<ConditionalHeader id="ml_5ofKind" />
			</ConditionalBag>
			
		</ConditionalHeaders>
		
		<RandomHeaders>
			<RandomBag id="dur2" duration="2" spinsToTrigger="2">
				<!-- id is the ID of the MovieClips Export linkage and the ID as used in the Art Asset Controller -->
				<RandomHeader id="ml_AllWinsPay" />
				<RandomHeader id="ml_win4Extra" />
			</RandomBag>
			
		</RandomHeaders>
		
		<Priority Queue="5ofKind" />
	</ModalHeaders>
	
	<LokiEyeHeaders>
		<ModalHeaders>
			<DefaultHeader id="lokiEyesNormal" />
			
			<MethodCallHeaders>
				<MethodCallBag id="onSpin">
					<!-- id is the ID of the MovieClips Export linkage and the ID as used in the Art Asset Controller -->
					<MethodCallHeader id="lokiEyesDown" />
					<!-- <MethodCallHeader id="ml_win4Extra" /> -->
				</MethodCallBag>
				
				<MethodCallBag id="noWin">
					<MethodCallHeader id="lokiEyesNormal" />
				</MethodCallBag>
				
				<MethodCallBag id="smallWin">
					<MethodCallHeader id="lokiEyesWin" />
				</MethodCallBag>
				
				<MethodCallBag id="bigWin">
					<MethodCallHeader id="lokiEyesSurprise" />
				</MethodCallBag>
				
				<MethodCallBag id="magicWin">
					<MethodCallHeader id="lokiEyesTrance" />
				</MethodCallBag>
				
			</MethodCallHeaders>
		</ModalHeaders>
	</LokiEyeHeaders>
	
	<commands>
		<flashSymbol interval="250" repeat="3">
			<animating_symbols continuos="0,1,2,3,4,5,6,13,14" />
		</flashSymbol>
		<reelspin delayBetweenReelStarts="200,500,1000,500,200">
			<breathe 	increments="-4,-8,-12,-8,-4,-2"			blurY="0" />
			<spin 		increments="40,-60,80,-60,40" 		blurYs="8,8,8,8,8" offSets="10,10,10,10,10" />
			<bounce 	increments="24,-4,-4,-4,-4,-4"	 	blurY="0" />
			<quickbounce 	increments="4" 	blurY="0" />
		</reelspin>
	</commands>
	
	<NumericFormatter space="10" /> <!-- space in pixels -->	
</SlotMachine>